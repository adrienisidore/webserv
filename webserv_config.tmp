root ./ressources;
autoindex off;
server
listen 127.0.0.1:8080;
root ./ressources;
index index.html;
client_max_body_size 8000;
client_header_buffer_size 1000;
request_header_timeout 3;
request_body_timeout 6;
keepalive_timeout 5;
send_timeout 10;
cgi_timeout 3;
location /colorfull/
autoindex on;
cgi_handler .py /usr/bin/python3;
location /
index index.html;
location /oui
return 301 /colorfull/;
index index.html;
